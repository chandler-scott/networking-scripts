$TTL 86400
@ IN SOA ns1.networking.local. admin.networking.local. (
1      ; Serial
604800 ; Refresh
86400  ; Retry
2419200 ; Expire
86400 ) ; Minimum TTL
;
@            IN   NS   ns1.networking.local.
ns1          IN   A    ${DNS_SERVER_IP}
dns-server   IN   A    ${DNS_SERVER_IP}
web-server   IN   A    ${WEB_SERVER_IP}
image-server IN   A    ${IMAGE_SERVER_IP}

